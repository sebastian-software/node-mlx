{
  "name": "@node-mlx/hf2swift",
  "version": "0.1.0",
  "description": "HuggingFace Transformers to MLX Swift code generator",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "hf2swift": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "py-ast": "^1.9.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.0",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "huggingface",
    "transformers",
    "swift",
    "mlx",
    "code-generator"
  ],
  "license": "MIT"
}
